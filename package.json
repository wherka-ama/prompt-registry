        }
      ]
    },
    "views": {
      "promptRegistry": [
        {
          "id": "promptregistry.marketplace",
          "name": "Marketplace",
          "type": "webview",
          "icon": "$(extensions)",
          "contextualTitle": "Prompt Marketplace"
        },
        {
          "id": "promptRegistryExplorer",
          "name": "Registry Explorer",
          "icon": "$(list-tree)",
          "contextualTitle": "Prompt Registry"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "promptregistry.selectVersion"
        },
        {
          "command": "promptregistry.checkUpdates"
        },
        {
          "command": "promptregistry.update"
        },
        {
          "command": "promptregistry.showVersion"
        },
        {
          "command": "promptregistry.showHelp"
        },
        {
          "command": "promptregistry.validateAccess"
        },
        {
          "command": "promptregistry.forceGitHubAuth"
        }
      ],
      "view/title": [
        {
          "command": "promptRegistry.toggleProfileView",
          "when": "view == promptRegistryExplorer",
          "group": "navigation@0"
        },
        {
          "command": "promptRegistry.exportSettings",
          "when": "view == promptRegistryExplorer",
          "group": "navigation@1"
        },
        {
          "command": "promptRegistry.importSettings",
          "when": "view == promptRegistryExplorer",
          "group": "navigation@2"
        },
        {
          "command": "promptRegistry.openSettings",
          "when": "view == promptRegistryExplorer",
          "group": "navigation@3"
        },
        {
          "command": "promptregistry.switchHub",
          "when": "view == promptRegistryExplorer",
          "group": "navigation@4"
        },
        {
          "command": "promptregistry.exportHubConfig",
          "when": "view == promptRegistryExplorer",
          "group": "navigation@5"
        },
        {
          "command": "promptregistry.openHubRepository",
          "when": "view == promptRegistryExplorer",
          "group": "navigation@6"
        }
      ],
      "view/item/context": [
        {
          "command": "promptRegistry.activateProfile",
          "when": "view == promptRegistryExplorer && (viewItem == profile || viewItem == hub_profile)",
          "group": "promptRegistry@1"
        },
        {
          "command": "promptRegistry.deactivateProfile",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^profile(-active)?$/ || viewItem == hub_profile)",
          "group": "promptRegistry@2"
        },
        {
          "command": "promptRegistry.editProfile",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^profile(-active)?$/ || viewItem == hub_profile)",
          "group": "promptRegistry@3"
        },
        {
          "command": "promptRegistry.exportProfile",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^profile(-active)?$/ || viewItem == hub_profile)",
          "group": "promptRegistry@4"
        },
        {
          "command": "promptRegistry.deleteProfile",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^profile(-active)?$/ || viewItem == hub_profile)",
          "group": "promptRegistry@5"
        },
        {
          "command": "promptRegistry.toggleProfileFavorite",
          "when": "view == promptRegistryExplorer && viewItem == hub_profile",
          "group": "promptRegistry@6"
        },
        {
          "command": "promptRegistry.viewBundle",
          "when": "view == promptRegistryExplorer && viewItem == installed_bundle",
          "group": "bundleActions@1"
        },
        {
          "command": "promptRegistry.updateBundle",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^installed_bundle_auto_disabled/ || viewItem =~ /^installed_bundle_auto_enabled/)",
          "group": "bundleActions@2"
        },
        {
          "command": "promptRegistry.updateBundle",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^installed_bundle_updatable_auto_disabled/ || viewItem =~ /^installed_bundle_updatable_auto_enabled/)",
          "group": "bundleActions@1"
        },
        {
          "command": "promptRegistry.checkBundleUpdates",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^installed_bundle_auto_disabled/ || viewItem =~ /^installed_bundle_auto_enabled/)",
          "group": "bundleActions@3"
        },
        {
          "command": "promptRegistry.enableAutoUpdate",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^installed_bundle_auto_disabled/ || viewItem =~ /^installed_bundle_updatable_auto_disabled/)",
          "group": "autoUpdate@1"
        },
        {
          "command": "promptRegistry.disableAutoUpdate",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^installed_bundle_auto_enabled/ || viewItem =~ /^installed_bundle_updatable_auto_enabled/)",
          "group": "autoUpdate@1"
        },
        {
          "command": "promptRegistry.uninstallBundle",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^installed_bundle_auto_disabled/ || viewItem =~ /^installed_bundle_auto_enabled/)",
          "group": "bundleActions@4"
        },
        {
          "command": "promptRegistry.uninstallBundle",
          "when": "view == promptRegistryExplorer && (viewItem =~ /^installed_bundle_updatable_auto_disabled/ || viewItem =~ /^installed_bundle_updatable_auto_enabled/)",
          "group": "bundleActions@4"
        },
        {
          "command": "promptRegistry.editSource",
          "when": "view == promptRegistryExplorer && viewItem == source",
          "group": "sourceActions@1"
        },
        {
          "command": "promptRegistry.syncSource",
          "when": "view == promptRegistryExplorer && viewItem == source",
          "group": "sourceActions@2"
        },
        {
          "command": "promptRegistry.removeSource",
          "when": "view == promptRegistryExplorer && viewItem == source",
          "group": "sourceActions@3"
        },
        {
          "command": "promptRegistry.toggleSource",
          "when": "view == promptRegistryExplorer && viewItem == source",
          "group": "sourceActions@4"
        },
        {
          "command": "promptregistry.syncHub",
          "when": "view == promptRegistryExplorer && viewItem == hub",
          "group": "hubActions@1"
        },
        {
          "command": "promptregistry.deleteHub",
          "when": "view == promptRegistryExplorer && viewItem == hub",
          "group": "hubActions@2"
        },
        {
          "command": "promptregistry.openItemRepository",
          "when": "view == promptRegistryExplorer && viewItem == hub",
          "group": "hubActions@3"
        },
        {
          "command": "promptregistry.openItemRepository",
          "when": "view == promptRegistryExplorer && viewItem == hub_profile",
          "group": "promptRegistry@7"
        },
        {
          "command": "promptregistry.openItemRepository",
          "when": "view == promptRegistryExplorer && viewItem == source",
          "group": "sourceActions@5"
        },
        {
          "command": "promptregistry.openItemRepository",
          "when": "view == promptRegistryExplorer && viewItem == installed_bundle",
          "group": "bundleActions@5"
        },
        {
          "command": "promptRegistry.moveToRepositoryCommit",
          "when": "view == promptRegistryExplorer && viewItem =~ /^installed_bundle.*_user$/",
          "group": "scopeActions@1"
        },
        {
          "command": "promptRegistry.moveToRepositoryLocalOnly",
          "when": "view == promptRegistryExplorer && viewItem =~ /^installed_bundle.*_user$/",
          "group": "scopeActions@2"
        },
        {
          "command": "promptRegistry.moveToUser",
          "when": "view == promptRegistryExplorer && viewItem =~ /^installed_bundle.*_repository_(commit|local_only)$/",
          "group": "scopeActions@1"
        },
        {
          "command": "promptRegistry.switchToLocalOnly",
          "when": "view == promptRegistryExplorer && viewItem =~ /^installed_bundle.*_repository_commit$/",
          "group": "scopeActions@2"
        },
        {
          "command": "promptRegistry.switchToCommit",
          "when": "view == promptRegistryExplorer && viewItem =~ /^installed_bundle.*_repository_local_only$/",
          "group": "scopeActions@2"
        }
      ]
    },
    "configuration": {
      "title": "Prompt Registry",
      "properties": {
        "promptregistry.autoCheckUpdates": {
          "type": "boolean",
          "default": true,
          "description": "Automatically check for updates on extension activation"
        },
        "promptregistry.installationScope": {
          "type": "string",
          "enum": [
            "user",
            "workspace",
            "project"
          ],
          "default": "user",
          "description": "Default installation scope for Prompt Registry components"
        },
        "promptregistry.enableLogging": {
          "type": "boolean",
          "default": true,
          "description": "Enable detailed logging for debugging purposes"
        },
        "promptregistry.githubApiUrl": {
          "type": "string",
          "default": "https://api.github.com",
          "description": "GitHub API URL for fetching releases"
        },
        "promptregistry.repositoryOwner": {
          "type": "string",
          "default": "AmadeusITGroup",
          "description": "GitHub repository owner"
        },
        "promptregistry.repositoryName": {
          "type": "string",
          "default": "prompt-registry",
          "description": "GitHub repository name"
        },
        "promptregistry.githubToken": {
          "type": "string",
          "default": "",
          "description": "GitHub personal access token for private repositories (generate with 'gh auth token')"
        },
        "promptregistry.usePrivateRepository": {
          "type": "boolean",
          "default": false,
          "description": "Enable access to private GitHub repositories using authentication"
        },
        "promptregistry.useGitHubCli": {
          "type": "boolean",
          "default": false,
          "description": "Use GitHub CLI (gh) for automatic token generation instead of manual token"
        },
        "promptregistry.defaultVersion": {
          "type": "string",
          "default": "latest",
          "description": "Default version to install (use 'latest' for latest release or specific version like 'v1.0.0')"
        },
        "promptregistry.updateCheck.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic update checks for installed bundles"
        },
        "promptregistry.updateCheck.frequency": {
          "type": "string",
          "enum": [
            "daily",
            "weekly",
            "manual"
          ],
          "default": "daily",
          "description": "How often to check for bundle updates"
        },
        "promptregistry.updateCheck.notificationPreference": {
          "type": "string",
          "enum": [
            "all",
            "critical",
            "none"
          ],
          "default": "all",
          "description": "Which updates to show notifications for"
        },
        "promptregistry.updateCheck.autoUpdate": {
          "type": "boolean",
          "default": false,
          "description": "Automatically install updates in the background"
        },
        "promptregistry.updateCheck.cacheTTL": {
          "type": "number",
          "default": 300000,
          "minimum": 60000,
          "maximum": 3600000,
          "description": "Cache time-to-live for update check results in milliseconds (default: 5 minutes, min: 1 minute, max: 1 hour)"
        }
      }
    }
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "package:vsix": "vsce package",
    "compile-tests": "tsc -p tsconfig.test.json && node scripts/copy-test-fixtures.js",
    "watch-tests": "tsc -p tsconfig.test.json -w",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "test": "npm run test:all",
    "test:one": "node scripts/run-single-test.js",
    "test:unit": "npx mocha --ui tdd --require ./test/mocha.setup.js --require ./test/unit.setup.js 'test-dist/test/{adapters,commands,config,helpers,services,storage,ui,unit,utils}/**/*.test.js' --timeout 5000",
    "test:integration": "npm run compile-tests && node ./test/runExtensionTests.js",
    "test:all": "npm run compile-tests && npm run test:unit && npm run test:integration",
    "test:coverage": "npm run compile-tests && c8 npm run test:all",
    "test:coverage:report": "c8 report --reporter=html --reporter=text --reporter=json",
    "test:coverage:unit": "npm run compile-tests && c8 --reporter=html --reporter=text mocha --ui tdd --require ./test/mocha.setup.js --require ./test/unit.setup.js 'test-dist/test/{adapters,commands,config,helpers,services,storage,ui,unit,utils}/**/*.test.js' --timeout 5000",
    "test:coverage:integration": "npm run compile-tests && c8 --reporter=html --reporter=text npm run test:integration",
    "coverage:clean": "rimraf coverage .nyc_output .c8_output",
    "ignore:status": "node scripts/package-helpers.js status",
    "ignore:dev": "node scripts/package-helpers.js dev",
    "ignore:prod": "node scripts/package-helpers.js prod",
    "ignore:restore": "node scripts/package-helpers.js restore",
    "package:prepare": "node scripts/package-helpers.js prod && npm run compile",
    "package:cleanup": "node scripts/package-helpers.js dev",
    "package:full": "npm run package:prepare && npm run package:vsix && npm run package:cleanup",
